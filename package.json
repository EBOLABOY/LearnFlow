{
  "name": "deeplearn-assistant",
  "version": "3.2.0",
  "description": "智能识别课程页面，自动完成视频播放与章节测试，解放您的时间",
  "private": true,
  "scripts": {
    "build": "cross-env BUILD_ENV=development node scripts/build.mjs",
    "build:prod": "cross-env BUILD_ENV=production node scripts/build.mjs",
    "pack": "npm run build:prod && node build/pack.js",
    "sourcemaps:upload": "node scripts/sentry-upload.js",
    "release": "standard-version",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true}); require('fs').rmSync('release', {recursive: true, force: true});\"",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@sentry/cli": "^2.53.0",
    "archiver": "^6.0.1",
    "babel-jest": "^30.1.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.9",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "standard-version": "^9.5.0",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "@sentry/browser": "^10.10.0",
    "bcryptjs": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.5"
  }
}
